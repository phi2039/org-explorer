{
  "name": "org-explorer",
  "productName": "Org  Explorer",
  "version": "1.0.6",
  "description": "Labor analysis tool",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "commit": "git-cz",
    "release": "standard-version",
    "release:tags": "git push --follow-tags release master",
    "prerelease": "git checkout master && git pull release master && yarn install && git add .",
    "postrelease": "run-s release:*",
    "ci:validate": "rm -rf node_modules && npm ci && npm run validate",
    "prepublishOnly": "npm run ci:validate"
  },
  "keywords": [],
  "author": {
    "name": "chris.lance",
    "email": "chris.lance@changehealthcare.com"
  },
  "license": "ISC",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "org_explorer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "phi2039",
              "name": "org-explorer"
            },
            "prerelease": true
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/electron/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-react": "^7.9.0",
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@welldone-software/why-did-you-render": "^4.0.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "electron": "^10.1.5",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "git-cz": "^4.7.4",
    "husky": "^4.3.0",
    "node-loader": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "standard-version": "^9.0.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@hot-loader/react-dom": "^16.13.0",
    "constate": "^2.0.0",
    "cuid": "^2.1.8",
    "electron-context-menu": "^0.12.1",
    "electron-is-dev": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^3.2.0",
    "eventemitter3": "^4.0.7",
    "formik": "^2.2.5",
    "immer": "^7.0.0",
    "is-electron": "^2.2.0",
    "is-url": "^1.2.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "node-yaml": "^4.0.1",
    "normalizr": "^3.3.0",
    "path-browserify": "^1.0.1",
    "predicate": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.13.0",
    "react-file-reader": "^1.1.4",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.20",
    "react-hotkeys-hook": "^2.4.0",
    "react-icons": "^3.6.1",
    "react-indiana-drag-scroll": "^1.6.0",
    "react-modal": "^3.11.2",
    "react-planet": "^1.0.1",
    "react-spinners": "^0.9.0",
    "read-excel-file": "^4.0.2",
    "styled-components": "^4.2.0",
    "styled-system": "^5.1.5",
    "yup": "^0.29.3"
  }
}
